  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "frontend",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}/frontend"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "backend",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/backend/index.mjs",
      "cwd": "${workspaceFolder}/backend"
    }
  ]
}
